<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ClientController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane fx:id="anchorp" prefHeight="480.0" prefWidth="640.0">
         <children>
            <Button fx:id="backButton" layoutX="597.0" layoutY="441.0" mnemonicParsing="false" onAction="#goBack" text="&lt;" />
            <Button fx:id="dsgbutton" layoutX="23.0" layoutY="27.0" mnemonicParsing="false" onAction="#chooseDsg" text="Choose" />
            <Label fx:id="pathLabel" layoutX="99.0" layoutY="27.0" prefHeight="25.0" prefWidth="527.0" text="C:/Path" />
            <TextField fx:id="filename" layoutX="23.0" layoutY="78.0" />
            <Label layoutX="23.0" layoutY="63.0" text="After decryption file name" />
            <Button layoutX="23.0" layoutY="177.0" mnemonicParsing="false" onAction="#decryptFile" text="Decrypt" />
            <ListView fx:id="receiversList" layoutX="320.0" layoutY="14.0" prefHeight="200.0" prefWidth="200.0" />
            <Button fx:id="connectionButton" layoutX="452.0" layoutY="441.0" mnemonicParsing="false" onAction="#checkConnection" text="Check Connection" />
            <Button fx:id="test" layoutX="27.0" layoutY="339.0" mnemonicParsing="false" onAction="#testAction" text="Test" />
         </children></AnchorPane>
   </children>
</GridPane>
